{
  "extends": "./node_modules/@1stg/tsconfig/node",
  "ts-node": {
    "transpileOnly": true
  },
  "include": ["src", "test"],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "paths": {
      "markuplint-sync": ["./src"],
      "@markuplint/ejs-parser": [
        "./markuplint/packages/@markuplint/ejs-parser/src"
      ],
      "@markuplint/ejs-parser/lib/*": [
        "./markuplint/packages/@markuplint/ejs-parser/src/*"
      ],
      "@markuplint/erb-parser": [
        "./markuplint/packages/@markuplint/erb-parser/src"
      ],
      "@markuplint/erb-parser/lib/*": [
        "./markuplint/packages/@markuplint/erb-parser/src/*"
      ],
      "@markuplint/file-resolver": [
        "./markuplint/packages/@markuplint/file-resolver/src"
      ],
      "@markuplint/file-resolver/lib/*": [
        "./markuplint/packages/@markuplint/file-resolver/src/*"
      ],
      "@markuplint/html-parser": [
        "./markuplint/packages/@markuplint/html-parser/src"
      ],
      "@markuplint/html-parser/lib/*": [
        "./markuplint/packages/@markuplint/html-parser/src/*"
      ],
      "@markuplint/html-spec": ["./markuplint/packages/@markuplint/html-spec"],
      "@markuplint/i18n": ["./markuplint/packages/@markuplint/i18n/src"],
      "@markuplint/i18n/lib/*": [
        "./markuplint/packages/@markuplint/i18n/src/*"
      ],
      "@markuplint/liquid-parser": [
