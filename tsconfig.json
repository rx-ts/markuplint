      ],
      "@markuplint/liquid-parser/lib/*": [
        "./markuplint/packages/@markuplint/liquid-parser/src/*"
      ],
      "@markuplint/ml-ast": ["./markuplint/packages/@markuplint/ml-ast/src"],
      "@markuplint/ml-ast/lib/*": [
        "./markuplint/packages/@markuplint/ml-ast/src/*"
      ],
      "@markuplint/ml-config": [
        "./markuplint/packages/@markuplint/ml-config/src"
      ],
      "@markuplint/ml-config/lib/*": [
