      ],
      "@markuplint/vue-parser/lib/*": [
        "./markuplint/packages/@markuplint/vue-parser/src/*"
      ],
      "@markuplint/vue-spec": [
        "./markuplint/packages/@markuplint/vue-spec/src"
      ],
      "@markuplint/vue-spec/lib/*": [
        "./markuplint/packages/@markuplint/vue-spec/src/*"
      ],
      "markuplint": ["./markuplint/packages/markuplint/src"],
      "markuplint/lib/*": ["./markuplint/packages/markuplint/src/*"]
    }
  }
}
