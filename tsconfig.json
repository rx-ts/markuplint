      ],
      "@markuplint/mustache-parser": [
        "./markuplint/packages/@markuplint/mustache-parser/src"
      ],
      "@markuplint/mustache-parser/lib/*": [
        "./markuplint/packages/@markuplint/mustache-parser/src/*"
      ],
      "@markuplint/nunjucks-parser": [
        "./markuplint/packages/@markuplint/nunjucks-parser/src"
      ],
      "@markuplint/nunjucks-parser/lib/*": [
        "./markuplint/packages/@markuplint/nunjucks-parser/src/*"
      ],
      "@markuplint/parser-utils": [
