      "@markuplint/pug-parser": [
        "./markuplint/packages/@markuplint/pug-parser/src"
      ],
      "@markuplint/pug-parser/lib/*": [
        "./markuplint/packages/@markuplint/pug-parser/src/*"
      ],
      "@markuplint/rule-textlint": [
        "./markuplint/packages/@markuplint/rule-textlint/src"
      ],
      "@markuplint/rule-textlint/lib/*": [
        "./markuplint/packages/@markuplint/rule-textlint/src/*"
      ],
      "@markuplint/rules": ["./markuplint/packages/@markuplint/rules/src"],
      "@markuplint/rules/lib/*": [
