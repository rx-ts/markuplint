{
  "name": "markuplint-sync",
  "version": "0.1.2",
  "description": "Same as markuplint but extends with sync API.",
  "repository": "git+https://github.com/rx-ts/markuplint.git",
  "homepage": "https://github.com/rx-ts/markuplint/tree/main/packages/sync",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "main": "lib",
  "module": "lib/es2015",
  "es2015": "lib/es2015",
  "types": "lib",
  "files": [
    "lib",
    "rules",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "markuplint",
    "markuplint-sync"
  ],
  "scripts": {
    "build": "rimraf rules && node scripts/patch-rules"
  },
  "dependencies": {
    "@markuplint/file-resolver": "^1.2.2",
    "@markuplint/html-spec": "^1.5.1",
    "@markuplint/i18n": "^1.1.1",
    "@markuplint/ml-ast": "^1.4.1",
    "@markuplint/ml-config": "^1.1.0",
    "@markuplint/ml-core": "^1.5.1",
    "@markuplint/ml-spec": "^1.4.1",
    "cosmiconfig": "^7.0.0",
    "glob": "^7.1.6",
    "markuplint": "^1.7.1",
    "os-locale": "^5.0.0",
    "tslib": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
