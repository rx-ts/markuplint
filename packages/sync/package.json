{
  "name": "markuplint-sync",
  "version": "0.3.3",
  "description": "Same as markuplint but extends with sync API.",
  "repository": "git+https://github.com/rx-ts/markuplint.git",
  "homepage": "https://github.com/rx-ts/markuplint/tree/main/packages/sync",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=v10"
  },
  "main": "lib/index.js",
  "module": "lib/index.es2017.mjs",
  "exports": {
    "import": "./lib/index.es2017.mjs",
    "require": "./lib/index.js"
  },
  "types": "lib",
  "files": [
    "lib",
    "rules",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "markuplint",
    "markuplint-sync"
  ],
  "scripts": {
    "build": "rimraf rules && node scripts/patch-rules"
  },
  "dependencies": {
    "@markuplint/file-resolver": "^1.5.0",
    "@markuplint/html-spec": "^1.8.2",
    "@markuplint/i18n": "^1.2.0",
    "@markuplint/ml-ast": "^1.6.0",
    "@markuplint/ml-config": "^1.3.0",
    "@markuplint/ml-core": "^1.9.0",
    "@markuplint/ml-spec": "^1.7.0",
    "cosmiconfig": "^7.0.1",
    "glob": "^7.2.0",
    "markuplint": "^1.11.4",
    "minimatch": "^3.0.4",
    "os-locale": "^5.0.0",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
